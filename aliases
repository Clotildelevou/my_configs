#aliases
  alias pi-connect='ssh -X ubuntu@192.168.1.22'
	alias ls='ls --color=auto'
	alias ll='ls -l --color=auto'
	alias la='ls -la --color=auto'
	alias grep='grep --colour=auto'
	alias egrep='egrep --colour=auto'
	alias fgrep='fgrep --colour=auto'
	alias zcc='gcc -Wall -Werror -Wextra -std=c99 -pedantic -fsanitize=address'
	alias z++='g++ -Wall -Werror -Wextra -std=c++17 -pedantic -fsanitize=address'
